<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.twig.finalproject" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.example.twig.androidActivities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.WelcomeActivity"
            android:label="Welcome Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.LoginActivity"
            android:label="Login Activity">
            android:parentActivityName="com.example.twig.androidActivities.WelcomeActivity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.ApplicationActivity"
            android:label="Application Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.RegisterActivity"
            android:label="Register Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.FriendListActivity"
            android:label="Friend List Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.FriendSearchActivity"
            android:label="Friend Search Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.FriendDetailActivity"
            android:label="Friend Detail Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.InterestListActivity"
            android:label="Interest List Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.RegisterInterestActivity"
            android:label="Register Interest Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.SalesListActivity"
            android:label="Sales List Activity">
        </activity>

        <activity
            android:name="com.example.twig.androidActivities.SalesReportActivity"
            android:label="Sales Report Activity">
        </activity>

    </application>

</manifest>
